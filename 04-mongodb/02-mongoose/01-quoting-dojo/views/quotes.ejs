    <body>
        <div id="container">

            Some initial content

            <h2>Here are the awesome quotes!</h2>
            <% for (var quote  of quotes) { %>
                <h3> <%= quote.quote %></h3>
                <h4>    -----: <%= quote.name %> at <%= quote.createdAt %>  </h4>
                <hr>
            <% } %>
        </div>
